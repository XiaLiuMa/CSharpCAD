using System;
using Dapper.Contrib.Extensions;

namespace DataSyncSource.Config.Entities
{
    [Table("User")]
    [Serializable]
    public class User
    {
        [Key]
        /// <summary>
        /// 主键Id
        /// </summary>
        public string Id { get; set; }
        /// <summary>
        /// 用户名
        /// </summary>
        public string Uname { get; set; }
        /// <summary>
        /// 密码
        /// </summary>
        public string Pword { get; set; }
        /// <summary>
        /// 权限(用逗号隔开)
        /// </summary>
        public string Permissions { get; set; }
    }
}
