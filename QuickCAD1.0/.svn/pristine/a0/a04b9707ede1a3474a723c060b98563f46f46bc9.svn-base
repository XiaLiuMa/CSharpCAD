using MsDataSimulator.Config;
using MsDataSimulator.Jobs;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace MsDataSimulator.Comm
{
    public class AsGlobal
    {
        #region 单例
        private static AsGlobal instance;
        private readonly static object objLock = new object();
        public static AsGlobal Instance
        {
            get
            {
                if (instance == null)
                {
                    lock (objLock)
                    {
                        if (instance == null)
                        {
                            instance = new AsGlobal();
                        }
                    }
                }
                return instance;
            }
        }
        #endregion

        public void Init()
        {
            CfgHandler.Instance.Init();
            UnitData.Instance.Init();
        }

        public void UnInit()
        {

        }
    }
}
