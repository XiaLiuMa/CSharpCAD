<body>
    <div class="layui-form layuimini-form">
        <textarea id="logDetail" name="logDetail" class="layui-textarea" readonly style="min-height:600px"></textarea>
    </div>
    </div>
    <script src="/lib/jquery-3.4.1/jquery.js" charset="utf-8"></script>
    <script src="/lib/layui-v2.9.2/layui.js" charset="utf-8"></script>
    <script>
        var parentMod = parent.parentMod;//全局变量，子页面获取父页面传来的参数。
        layui.use(['form'], function () {
            var form = layui.form;
            var layer = layui.layer;
            var $ = layui.$;

            $.ajax({
                type: 'POST',
                url: '/LogMgt/ViewLog', //接口地址
                cache: false, //是否使用缓存
                dataType: "json",//数据类型
                data: { "fileName": parentMod.logFullFileName },
                success: function (res) {
                    if (res.code == 0) {
                        $("#logDetail").val(res.data);
