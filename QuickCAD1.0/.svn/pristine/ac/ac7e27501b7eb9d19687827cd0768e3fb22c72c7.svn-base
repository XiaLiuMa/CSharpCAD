namespace SetCharLib
{
    public class GlobalSetChar
    {
        #region 单例
        private static GlobalSetChar i;
        private readonly static object objLock = new object();
        public static GlobalSetChar I
        {
            get
            {
                if (i == null)
                {
                    lock (objLock)
                    {
                        if (i == null)
                        {
                            i = new GlobalSetChar();
                        }
                    }
                }
                return i;
            }
        }
        #endregion

